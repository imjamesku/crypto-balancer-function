    "build/src"
  ],
  "license": "Apache-2.0",
  "keywords": [],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "start": "yaml2json .env.local.yaml >.env.json && env-cmd -r ./.env.json functions-framework --source=build/src/ --target=helloWorld",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./build/ --exec npm run start\"",
    "deploy": "gcloud functions deploy rebalance --trigger-http --runtime=nodejs14 --entry-point helloWorld --env-vars-file .env.yaml"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^14.11.2",
    "concurrently": "^7.1.0",
    "env-cmd": "^10.1.0",
    "gts": "^3.1.0",
    "nodemon": "^2.0.16",
    "typescript": "^4.0.3",
    "yaml2json": "^1.0.2"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.1.1"
  }
}
